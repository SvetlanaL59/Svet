# ДАНО Словарь: ключ - название банка : значение - процент
per_cent = {"ТКБ" : 5.6 , "СКБ" : 5.9 , "ВТБ" : 4.28 , "СБЕР" : 4.0 }
# print(per_cent)
# ВВОД Сумма, которую человек планирует положить под проценты
money=float(input("Введите сумму, которую вы планируете положить под проценты: "))

# формирование списка значений
# накопленных средств за год вклада в каждом из банков

deposit = [i * money / 100 for i in per_cent.values()]
print("money = ",money)
print("deposit = : ",deposit)

# поиск максимального элемента в списке
m = max(deposit)
# вывод результата
print("Максимальная сумма, которую вы можете заработать - ",m)
